        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Homework" Height="350" Width="350">
    <Grid>
        <Button x:Name="addSubjectButton" Content="Button" HorizontalAlignment="Left" Margin="246,10,0,0" VerticalAlignment="Top" Width="75" Click="addSubjectButton_Click"/>
        <StackPanel x:Name="homeworkPanel" Orientation="Horizontal" HorizontalAlignment="Left" Height="244" Margin="21,53,0,0" VerticalAlignment="Top" Width="300">
            <GroupBox x:Name="subjectBox" Header="GroupBox" Width="300"/>
        </StackPanel>
    </Grid>
</Window>

        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Quicknotes" Height="300" Width="300" Closing="onClosing" LostFocus="lostFocus" WindowStyle="None">
    <Grid>
        <TextBox x:Name="QuicknotesText" Margin="0" TextWrapping="Wrap" Text="Type notes here!&#xD;&#xA;" AcceptsReturn="True" AcceptsTab="True" KeyUp="onKeyUp"/>

    </Grid>
</Window>
